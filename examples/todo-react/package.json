{
  "name": "rep-example-todo-react",
  "version": "0.1.4",
  "private": true,
  "description": "Example: Todo app showing REP gateway + React SDK integration",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "rep:validate": "rep validate --manifest .rep.yaml",
    "rep:typegen": "rep typegen --manifest .rep.yaml --output src/rep.d.ts",
    "rep:lint": "rep lint --dir ./dist",
    "rep:dev": "rep dev --port 3000 --env .env.local --proxy http://localhost:5173 --hot-reload",
    "rep:serve": "rep dev --port 3000 --env .env.local --static ./dist --hot-reload"
  },
  "dependencies": {
    "@rep-protocol/react": "^0.1.4",
    "@rep-protocol/sdk": "^0.1.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@rep-protocol/cli": "^0.1.4",
    "typescript": "^5.4.0",
    "vite": "^5.4.0"
  }
}
